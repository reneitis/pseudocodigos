programa
{
	
//RENATA VIEIRA 
	funcao inicio()
	{
		real peso, altura, imc
		cadeia nome
		escreva("digite seu nome: ")
		leia(nome)
		escreva("digite seu peso: ")
		leia(peso)
		escreva("digite sua altura: ")
		leia(altura)
		imc = peso / (altura*altura)
		se (imc < 20){
			escreva (nome," está abaixo do peso")
		}
		senao se (imc <= 20 e imc <= 24.9){
			escreva (nome, "está com o peso normal")
		}
		senao se (imc < 25 e imc < 29.9){
			escreva(nome, "está sobrepeso")
		}
		senao se (imc < 30 e imc <= 39.9 ){
			escreva(nome, "está obeso")
		}
		senao se (imc >= 40){
			escreva(nome, "está obeso mórbido")
		}
	
	}
}
